<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>History and War TV Shows</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>war</value>
        <value>krieg</value>
        <value>history</value>
        <value>historie</value>
        <value>kriegsserie</value>
    </rule>
    <order direction="ascending">sorttitle</order>
</smartplaylist>
