<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- osdvideosettings -->
	<include name="IncludesOSDSettings">
		<!-- Default Video Settings -->
		<control type="group">
			<visible>String.IsEmpty(Window(home).Property(Show3DSettings))</visible>
			<top>655</top>
			<animation effect="fade" start="100" end="0" time="200" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
			<!-- Background -->
			<control type="image">
				<width>1920</width>
				<height>425</height>
				<texture>osd/mask.png</texture>
			</control>
			<!-- Heading label -->
			<control type="label" id="2">
				<left>92</left>
				<top>20</top>
				<height>36</height>
				<font>Bold30</font>
				<textcolor>ffdadada</textcolor>
			</control>
			<!-- Grouplist -->
			<control type="grouplist" id="5">
				<left>87</left>
				<top>60</top>
				<width>805</width>
				<height>348</height>
				<itemgap>0</itemgap>
				<onleft>noop</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<orientation>vertical</orientation>
				<scrolltime tween="sine" easing="out">240</scrolltime>
			</control>
			<!-- Default button -->
			<control type="button" id="7">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
			</control>
			<!-- Default radiobutton -->
			<control type="radiobutton" id="8">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<radioposx>708</radioposx>
				<radiowidth />
				<radioheight />
				<textureradioonfocus>-</textureradioonfocus>
				<textureradioonnofocus>-</textureradioonnofocus>
				<textureradioofffocus>-</textureradioofffocus>
				<textureradiooffnofocus>-</textureradiooffnofocus>
			</control>
			<!-- Default spincontrolex -->
			<control type="spincontrolex" id="9">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
			</control>
			<!-- Default sliderex -->
			<control type="sliderex" id="13">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<sliderwidth>200</sliderwidth>
				<sliderheight>11</sliderheight>
			</control>
		</control>
		<!-- 3D Video Settings -->
		<control type="group">
			<visible>!String.IsEmpty(Window(home).Property(Show3DSettings))</visible>
			<top>825</top>
			<animation effect="fade" start="100" end="0" time="200" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser) | Window.IsVisible(selectdialog)">Conditional</animation>
			<!-- Background -->
			<control type="image">
				<width>1920</width>
				<height>425</height>
				<texture>osd/mask.png</texture>
			</control>
			<!-- Heading label -->
			<control type="label">
				<left>92</left>
				<top>20</top>
				<height>36</height>
				<font>Bold30</font>
				<label>$LOCALIZE[31153]</label>
				<textcolor>ffdadada</textcolor>
			</control>
			<!-- Grouplist -->
			<control type="grouplist" id="5">
				<left>87</left>
				<top>60</top>
				<width>805</width>
				<height>348</height>
				<itemgap>0</itemgap>
				<onleft>noop</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<orientation>vertical</orientation>
				<scrolltime tween="sine" easing="out">240</scrolltime>
				<control type="radiobutton" id="501">
					<height>58</height>
					<font>Light32</font>
					<textoffsetx>5</textoffsetx>
					<textcolor>ffffffff</textcolor>
					<shadowcolor>ff000000</shadowcolor>
					<focusedcolor>ffffc04b</focusedcolor>
					<disabledcolor>88ffffff</disabledcolor>
					<texturefocus>-</texturefocus>
					<texturenofocus>-</texturenofocus>
					<radioposx>708</radioposx>
					<radiowidth />
					<radioheight />
					<textureradioonfocus>-</textureradioonfocus>
					<textureradioonnofocus>-</textureradioonnofocus>
					<textureradioofffocus>-</textureradioofffocus>
					<textureradiooffnofocus>-</textureradiooffnofocus>
					<label>$LOCALIZE[31255]</label>
					<onclick>ToggleStereoMode</onclick>
					<selected>Integer.IsGreater(System.StereoscopicMode,0)</selected>
				</control>
				<control type="radiobutton" id="503">
					<height>58</height>
					<font>Light32</font>
					<textoffsetx>5</textoffsetx>
					<textcolor>ffffffff</textcolor>
					<shadowcolor>ff000000</shadowcolor>
					<focusedcolor>ffffc04b</focusedcolor>
					<disabledcolor>88ffffff</disabledcolor>
					<texturefocus>-</texturefocus>
					<texturenofocus>-</texturenofocus>
					<radioposx>708</radioposx>
					<radiowidth />
					<radioheight />
					<textureradioonfocus>-</textureradioonfocus>
					<textureradioonnofocus>-</textureradioonnofocus>
					<textureradioofffocus>-</textureradioofffocus>
					<textureradiooffnofocus>-</textureradiooffnofocus>
					<label>$LOCALIZE[31256]</label>
					<onclick>StereoModeToMono</onclick>
					<selected>String.IsEqual(System.StereoscopicMode,9)</selected>
				</control>
				<control type="button" id="502">
					<height>58</height>
					<font>Light32</font>
					<textoffsetx>5</textoffsetx>
					<textcolor>ffffffff</textcolor>
					<shadowcolor>ff000000</shadowcolor>
					<focusedcolor>ffffc04b</focusedcolor>
					<disabledcolor>88ffffff</disabledcolor>
					<texturefocus>-</texturefocus>
					<texturenofocus>-</texturenofocus>
					<label>$LOCALIZE[31257]</label>
					<onclick>StereoMode</onclick>
				</control>
			</control>
		</control>
	</include>
</includes>