<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- sliderdialog -->
	<defaultcontrol always="true">11</defaultcontrol>
	<controls>
		<!-- Main group -->
		<control type="group">
			<include>DialogAnimation</include>
			<right>12</right>
			<top>12</top>
			<width>800</width>
			<height>224</height>
			<!-- Hidden Control -->
			<control id="32" type="progress">
				<top>-100</top>
			</control>
			<!-- Background image -->
			<control type="image">
				<width>800</width>
				<height>224</height>
				<texture border="48">views/shadow_fo.png</texture>
			</control>
			<control type="image">
				<left>48</left>
				<top>48</top>
				<width>704</width>
				<height>128</height>
				<texture border="4">dialogs/DialogNotificationBackground.png</texture>
			</control>
			<!-- icon -->
			<control type="group">
				<left>54</left>
				<top>54</top>
				<width>116</width>
				<height>116</height>
				<!-- Icon back -->
				<control type="image">
					<width>116</width>
					<height>116</height>
					<texture>dialogs/progress/bg.png</texture>
				</control>
				<control type="image">
					<width>116</width>
					<height>116</height>
					<texture>dialogs/progress/bg_circle.png</texture>
				</control>
				<!-- Icon -->
				<control type="image">
					<width>116</width>
					<height>116</height>
					<texture>dialogs/icons/slider.png</texture>
				</control>
			</control>
			<!-- Heading -->
			<control id="10" type="label">
				<left>182</left>
				<top>72</top>
				<width>552</width>
				<height>44</height>
				<font>Med30</font>
				<scroll>true</scroll>
				<textcolor>ffdadada</textcolor>
			</control>
			<control id="12" type="label">
				<left>182</left>
				<top>72</top>
				<align>right</align>
				<width>552</width>
				<height>44</height>
				<font>Light30</font>
				<scroll>true</scroll>
				<textcolor>fffbbb45</textcolor>
			</control>
			<!-- Details -->
			<control id="11" type="slider">
				<left>182</left>
				<top>130</top>
				<width>552</width>
				<height>11</height>
				<orientation>horizontal</orientation>
				<texturesliderbar border="4">osd/progress_back.png</texturesliderbar>
				<textureslidernib border="4">osd/progress_cache.png</textureslidernib>
				<textureslidernibfocus border="4">osd/progress_bar.png</textureslidernibfocus>
			</control>
		</control>
	</controls>
</window>