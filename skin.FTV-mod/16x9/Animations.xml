		<animation delay="200" easing="inout" effect="fade" end="100" start="0" time="120" tween="sine">Visible</animation>
		<animation effect="fade" end="0" start="100" time="0">Hidden</animation>
	</include>
	<!-- Window heading delay animation -->
	<include name="LabelsDelayFadeAnimation">
		<animation delay="400" easing="inout" effect="fade" end="100" start="0" time="200" tween="sine">Visible</animation>
		<animation effect="fade" end="0" start="100" time="0">Hidden</animation>		
		<animation delay="400" effect="fade" end="100" start="0" time="200">WindowOpen</animation>
	</include>
	<!-- Notification fade animation -->
	<include name="NotificationAnimation">
		<animation easing="inout" effect="slide" end="0,0" start="0,-111" time="120" tween="sine">WindowOpen</animation>
		<animation easing="inout" effect="slide" end="0,-111" start="0,0" time="120" tween="sine">WindowClose</animation>
	</include>
	<!-- Keyboard fade animation -->
	<include name="KeyboardAnimation">
		<animation easing="inout" effect="slide" end="0,0" start="0,-700" time="120" tween="sine" condition="!String.IsEmpty(Window(Home).Property(DisableSuggestions)) | !Integer.IsGreater(Container(9091).NumItems,0)">WindowOpen</animation>
		<animation easing="inout" effect="slide" end="0,0" start="0,-1145" time="120" tween="sine" condition="String.IsEmpty(Window(Home).Property(DisableSuggestions)) + Integer.IsGreater(Container(9091).NumItems,0)">WindowOpen</animation>
		<animation easing="inout" effect="slide" end="0,-700" start="0,0" time="120" tween="sine" condition="!String.IsEmpty(Window(Home).Property(DisableSuggestions)) | !Integer.IsGreater(Container(9091).NumItems,0)">WindowClose</animation>
		<animation easing="inout" effect="slide" end="0,-1145" start="0,0" time="120" tween="sine" condition="String.IsEmpty(Window(Home).Property(DisableSuggestions)) + Integer.IsGreater(Container(9091).NumItems,0)">WindowClose</animation>
		<animation easing="inout" effect="slide" end="0,-700" start="0,0" time="120" tween="sine" condition="[!String.IsEmpty(Window(Home).Property(DisableSuggestions)) | !Integer.IsGreater(Container(9091).NumItems,0)] + Window.IsVisible(numericinput)">Conditional</animation>
		<animation easing="inout" effect="slide" end="0,-1145" start="0,0" time="120" tween="sine" condition="String.IsEmpty(Window(Home).Property(DisableSuggestions)) + Integer.IsGreater(Container(9091).NumItems,0) + Window.IsVisible(numericinput)">Conditional</animation>
	</include>
	<!-- Dialog fade animation -->
	<include name="DialogAnimation">
		<animation easing="inout" effect="fade" end="100" start="0" time="120" tween="sine">WindowOpen</animation>
		<animation easing="inout" effect="fade" end="0" start="100" time="120" tween="sine">WindowClose</animation>
	</include>
	<!-- Dialog fade and zoom animation -->
	<include name="DialogZoomAnimation">
		<animation type="WindowOpen">
			<effect center="auto" end="100" start="90" time="120" type="zoom" />
			<effect easing="inout" end="100" start="0" time="120" tween="sine" type="fade" />
		</animation>
		<animation type="WindowClose">
			<effect center="auto" end="90" start="100" time="120" type="zoom" />
			<effect easing="inout" end="0" start="100" time="120" tween="sine" type="fade" />
		</animation>
	</include>
	<!-- Dialog video info animation -->
	<include name="DialogVideoInfoAnimation">
		<animation condition="Control.HasFocus(50) | Control.HasFocus(150) | Control.HasFocus(250) | Control.HasFocus(1000) | Control.HasFocus(5000)" effect="fade" end="6" start="100" time="200">Conditional</animation>
	</include>
	<!-- Home Widgets Animation -->
	<include name="HomeWidgetsAnimation">
		<animation condition="!String.IsEmpty(Window(home).Property(ChangeAnimation)) | [!Control.IsVisible(31) + !Control.IsVisible(32)]" reversible="false" type="Visible">
			<effect center="960,540" delay="280" easing="in" end="100" start="98" time="180" tween="sine" type="zoom" />
			<effect delay="280" easing="in" end="100" start="0" time="180" tween="sine" type="fade" />
		</animation>
		<animation condition="String.IsEmpty(Window(home).Property(ChangeAnimation)) + Control.IsVisible(32)" reversible="false" type="Visible">
			<effect center="960,540" delay="280" easing="in" end="100" start="98" time="180" tween="sine" type="zoom" />
			<effect delay="280" easing="in" end="0,0" start="0,-40" time="180" tween="sine" type="slide" />
			<effect delay="280" easing="in" end="100" start="0" time="180" tween="sine" type="fade" />
		</animation>
		<animation condition="String.IsEmpty(Window(home).Property(ChangeAnimation)) + Control.IsVisible(31)" reversible="false" type="Visible">
			<effect center="960,540" delay="280" easing="in" end="100" start="98" time="180" tween="sine" type="zoom" />
			<effect delay="280" easing="in" end="0,0" start="0,40" time="180" tween="sine" type="slide" />
			<effect delay="280" easing="in" end="100" start="0" time="180" tween="sine" type="fade" />
		</animation>
		<animation reversible="false" type="Hidden">
			<effect center="960,540" easing="out" end="98" start="100" time="180" tween="linear" type="zoom" />
			<effect easing="out" end="0" start="100" time="180" tween="linear" type="fade" />
		</animation>
		<animation condition="Window.IsVisible(1101)" reversible="false" type="Conditional">
			<effect easing="out" end="0,30" start="0,0" time="60" tween="sine" type="slide" />
			<effect delay="80" easing="out" end="0,-30" start="0,0" time="120" tween="sine" type="slide" />
		</animation>
		<animation condition="Window.IsVisible(1102)" reversible="false" type="Conditional">
			<effect easing="out" end="0,-30" start="0,0" time="60" tween="sine" type="slide" />
			<effect delay="80" easing="out" end="0,30" start="0,0" time="120" tween="sine" type="slide" />
		</animation>
	</include>
	<!-- Home Message Widget Animation -->
	<include name="HomeMessageAnimation">
