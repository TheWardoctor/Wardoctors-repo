<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.stargate.guide" name="Stargate Guide"
version="1.0.8" provider-name="Wardoctor">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.dateutil" version="2.4.2" />
        <import addon="script.module.pil" />
        <import addon="script.module.beautifulsoup4" />
    </requires>
    <extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
    <extension point="xbmc.addon.metadata">
        <description lang="en">TV Guide especially for Stargate Streams. Based on Fullscreen TV Guide with major contributions from: primaeval, iainmacleod, im85288, RayW1986, bluezed, andy, Zomboided, tycoo, kjb86.</description>
        <disclaimer lang="en">Based on the original TV Guide addon by twinther, FTV Guide and Midraal's fork.</disclaimer>
        <language></language>
        <platform>all</platform>
        <license>GPLv2</license>
        <forum></forum>
        <source></source>
        <website></website>
        <email></email>
        <news>
        </news>
    </extension>
</addon>
